export plugname=stream_store
portbase=61020
DAEMONS 1
echo '{"job-id" : 10364, "rank" : 1, "kokkos-perf-data" : [ {"name" : "SPARTAFOO0", "count": 0, "time": 0.0000}]}' > $LDMSD_RUN/x.json
VGARGS="--leak-check=full --show-leak-kinds=definite --track-origins=yes --trace-children=yes"
#vgon
LDMSD 1
vgoff
echo "run -p $port1 -a none -s foo -t json -x sock -h localhost -f $LDMSD_RUN/x.json"
#gdb ldmsd_stream_publish
for i in $(seq 1); do
	SLEEP 1
	ldmsd_stream_publish -p $port1 -a none -s foo -t json -x sock -h localhost -f $LDMSD_RUN/x.json -r 1 -i 0.1
	echo publish done $i
done
ps guxw |grep ldmsd_stream_publish
SLEEP 10
KILL_LDMSD 1
file_created $STOREDIR/csv/foo.*

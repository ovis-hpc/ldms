msg_enable

load name=dstat
config name=dstat producer=localhost${i} instance=localhost${i}/${dstat_schema} component_id=${i} mmalloc=1 io=1 fd=1 auto-schema=1 stat=1) perm=777
start name=dstat interval=1000000 offset=0

load name=procstat
config name=procstat producer=localhost${i} instance=localhost${i}/procstat component_id=${i}
start name=procstat interval=1000000 offset=0

load name=vmstat
config name=vmstat producer=localhost${i} instance=localhost${i}/vmstat component_id=${i}
start name=vmstat interval=1000000 offset=0

load name=procnet
config name=procnet producer=localhost${i} instance=localhost${i}/procnet component_id=${i}
start name=procnet interval=1000000 offset=0

#export VGARGS="--track-origins=yes --leak-check=full"
LDMSD -p prolog.sampler 1
#vgon
LDMSD -p prolog.store2_createuid 2
#vgoff
MESSAGE ldms_ls on host 2:
LDMS_LS 1 -v
SLEEP 5
ls -l $STOREDIR/node/${testname}.*
